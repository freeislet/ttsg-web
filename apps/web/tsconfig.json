{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "types": ["./worker-configuration.d.ts", "node"],
    "baseUrl": ".",
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": false,
    "sourceMap": true,
    "inlineSourceMap": false,
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
